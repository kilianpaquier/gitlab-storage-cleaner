# Craft configuration file (https://github.com/kilianpaquier/craft)
# yaml-language-server: $schema=https://raw.githubusercontent.com/kilianpaquier/craft/beta/.schemas/craft.schema.json
bot: dependabot
ci:
  auth:
    release: github-token
  docker:
    registry: ghcr.io
  name: github
  options:
    - codecov
    - codeql
    - labeler
  release:
    auto: true
description: Easily clean gitlab maintained repositories storage (jobs artifacts only) with a simple command
license: mit
maintainers:
  - name: kilianpaquier
platform: github
